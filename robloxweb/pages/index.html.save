    <script>
      const socket = io();
      socket.on("new", (count) => {
        document.querySelector(".updateStatsOnline").innerText = count;
      });
      socket.on("quit", (count) => {
        document.querySelector(".updateStatsOnline").innerText = count;
      });
    </script>
    <script>
      fetch("/statistics")
        .then((res) => res.json())
        .then((res) => {
          document
            .querySelectorAll(".updateStatsBalanceAll")
            .forEach((elem) => {
              elem.innerText =
                Number(res.available) + Number(res.groupAvailable);
            });

          document.querySelector(".updateStatsAll").innerText = res.selled;
          document.querySelector(".updateStatsOnline").innerText =
            res.online + 1;
          document.querySelector(".updateCourseTransfer").innerText =
            res.exchangeRate;

          document.querySelector(".updateGroupCourseTransfer").innerText =
            res.groupExchangeRate;
        });
    </script>
